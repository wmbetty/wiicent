.sessionBox{
	margin-top: 44rpx;
	background-color: #fff;
	border-top: 1px solid #dddddd;
}
.sessionBox .session{
	width: 100%;
	height: 116rpx;
	border-bottom: 1px solid #dddddd;
	padding-left: 36rpx;
	padding-right: 36rpx;
	box-sizing: border-box;
}
.sessionBox .session .sessionL{
	width: 70%;
	height: 116rpx;
	float: left;
	line-height: 116rpx;
	font-size: 28rpx;
	color:#333333;
	text-align: right;
	padding-right: 36rpx;
	box-sizing: border-box;
	border-right: 1px solid #dddddd;
}
.sessionBox .session .sessionL image{
	width: 72rpx;
	height: 40rpx;
	float: left;
	margin-right: 20rpx;
	margin-top: 38rpx;
}
.sessionBox .session .sessionR{
	width: 30%;
	float: left;
	height: 116rpx;
}
.sessionBox .session input{
	width: 100%;
	height: 116rpx;
	line-height: 116rpx;
	font-size: 28rpx;
	color: #333333;
	padding-left:36rpx;
	box-sizing:border-box;
}
.des{
	width: 100%;
	height: 100rpx;
	line-height: 100rpx;
	text-align: center;
	font-size: 28rpx;
	color: #999999;
	margin-bottom:40rpx;
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
}
.btnArea .btn{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #09ba79;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
	border-radius: 20rpx;
}