}
.renyuanDetailBox textarea{
	width: 100%;
	height: 200rpx;
	line-height: 54rpx;
	padding: 20rpx;
	box-sizing: border-box;
	font-size: 28rpx;
	color: #333333;
	margin-top:30rpx;
  	background-color: #fff;
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 58rpx;
}
.btnArea button{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #09ba79;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
}
.btnArea1{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 30rpx;
}
.btnArea1 button{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #fff;
	text-align: center;
	font-size: 36rpx;
	color: #09ba79;
	border: 1px solid #09ba79;
	box-sizing: border-box;
}
.addIdentityBox{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	padding-top: 30rpx;
	box-sizing: border-box;
}
.addIdentityBox .identityBox input{
	width: 65%;
	height: 80rpx;
	line-height: 80rpx;
	border-radius: 10rpx;
	background-color: #fff;
	margin-right: 5%;
	font-size: 32rpx;
	color: #333333;
  	padding-left: 30rpx;
	box-sizing: border-box;
  	float: left;
  	outline: none;
  	border: 1px solid #dddddd;
}
.addIdentityBox .identityBox .addIdentity{
	width: 30%;
	height: 80rpx;
	line-height: 80rpx;
	border: 1px solid #dddddd;
	border-radius: 10rpx;
	background-color: #fff;
	font-size: 28rpx;
	color: #333333;
	box-sizing: border-box;
	text-align: center;
  	float: left;
}
.addIdentityBox .identityBox::after{
  content: ' ';
  display: block;
  height: 0;
  clear: both;
}
.addIdentityBox .identityListBox{
	width: 100%;
	margin-top: 32rpx;
}
.addIdentityBox .identityListBox::after{
  content: ' ';
  display: block;
  height: 0;
  clear: both;
}
.addIdentityBox .identityListBox .identityItem{
	width: 148rpx;
	height: 60rpx;
	float: left;
  	margin-right: 10rpx;
}
.addIdentityBox .identityListBox .identityItem .selectIdentity{
	width: 148rpx;
	height: 60rpx;
	line-height: 60rpx;
	border-radius: 30rpx;
	font-size: 26rpx;
	text-align: center;
	color: #fff;
	background-color: #09ba79;
	box-sizing: border-box;
}
.addIdentityBox .identityListBox .identityItem .unselectIdentity{
	width: 148rpx;
	height: 60rpx;
	line-height: 60rpx;
	border-radius: 30rpx;
	font-size: 26rpx;
	text-align: center;
	color: #09ba79;
	background-color: #fff;
	border: 1px solid #09ba79;
	box-sizing: border-box;
}
.addIdentityBox .addIdentityBtnBox button{
	width: 100%;
	margin-top: 76rpx;
	height: 94rpx;
	line-height: 94rpx;
	text-align:center;
	color: #fff;
	background-color: #09ba79;
}