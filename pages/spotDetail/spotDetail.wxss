.spotDetailBox{
	width: 100%;
}
.session label{
	width: 100%;
	padding-left: 30rpx;
	box-sizing: border-box;
	height: 68rpx;
	line-height: 68rpx;
	color: #999999;
	font-size: 28rpx;
	margin-top: 20rpx;
  display: block;
}
.session .itemBox{
	width: 100%;
	height: 90rpx;
	line-height: 90rpx;
	padding-left: 30rpx;
  padding-right: 30rpx;
	box-sizing: border-box;
	font-size: 28rpx;
	color: #999999;
	border-top: 1px solid #dddddd;
	border-bottom: 1px solid #dddddd;
  background-color: #fff;
  position:relative;
}
.session .itemBox .dateBox{
	float:left;
	height: 90rpx;
	line-height: 90rpx;
  	padding-right: 30rpx;
	font-size: 28rpx;
	color: #999999;
}
.session textarea{
	width: 100%;
	height: 400rpx;
	padding: 30rpx;
	box-sizing: border-box;
	font-size: 28rpx;
	color: #999999;
	border-top: 1px solid #dddddd;
	border-bottom: 1px solid #dddddd;
  background-color: #fff;
  over-flow:auto;
}
.session input{
	width: 100%;
	height: 86rpx;
	line-height:86rpx;
	font-size: 28rpx;
	color: #999999;
	box-sizing: border-box;
	font-size: 28rpx;
	color: #999999;
  background-color: #fff;
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 78rpx;
}
.btnArea view{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #fd8238;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
	border-radius: 20rpx;
	margin-bottom: 20rpx;
}
.btnArea .delBtn{
	background-color: #e94f4f;
}
.session .itemBox .arrowLogo{
	width: 34rpx;
	height: 34rpx;
	position: absolute;
	top: 24rpx;
	right: 30rpx;
	background-size: 34rpx 34rpx;
	background-repeat: no-repeat;
	background-image: url(data:image/png;base64,);
}