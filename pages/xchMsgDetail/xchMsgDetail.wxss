.lineBox{
  width: 100%;
  height: 86rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  border-bottom: 1px solid #dddddd;
  box-sizing: border-box;
  display: flex;
  flex-wrap: nowrap;
  background-color: #ffffff;
}
.lineBox .title{
  height: 86rpx;
  line-height: 86rpx;
  font-size: 28rpx;
  color: #333333;
  white-space: nowrap;
}
.lineBox .content{
  height: 86rpx;
  line-height: 86rpx;
  font-size: 28rpx;
  color: #333333;
}
.lineBox .green{
  color: #09ba79;
}
.lineWrap{
  margin-top: 20rpx;
  border-top: 1px solid #dddddd;
}
.lineBox .text{
  width: 100%;
  height: 86rpx;
  font-size: 28rpx;
  color: #333333;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.areaBox{
  width: 100%;
  padding: 20rpx 30rpx;
  box-sizing: border-box;
  font-size: 24rpx;
  color: #333333;
  border-top: 1px solid #dddddd;
  border-bottom: 1px solid #dddddd;
  background-color: #fff;
  margin-top:20rpx;
  line-height:46rpx;
}
.btnBox{
  width: 100%;
  padding-left: 30rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  margin-top: 54rpx;
  padding-bottom:200rpx;
}
.btnBox button{
  width: 100%;
  height: 94rpx;
  font-size: 34rpx;
  background-color: #09ba79;
  color: #fff;
  border-style: none;
}