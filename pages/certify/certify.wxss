page{
	background-color:#fff;
}
.tabBox{
	width: 100%;
	height: 200rpx;
}
.tabBox .itemList{
	width: 100%;
	height: 200rpx;
}
.panelList{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	padding-top: 30rpx;
	box-sizing: border-box;
}
.iconBox{
	width: 100%;
	position: relative;
	height: 100rpx;
	border: 1px solid #dddddd;
	border-radius: 12rpx;
	background-color: #fff;
	box-sizing: border-box;
}
.iconBox .album{
	position: absolute;
	top: 24rpx;
	right: 132rpx;
	height: 46rpx;
}
.iconBox .album image{
	width: 46rpx;
	height: 46rpx;
}
.iconBox .photo{
	position: absolute;
	top: 24rpx;
	right: 24rpx;
	height: 46rpx;
}
.iconBox .photo image{
	width: 46rpx;
	height: 46rpx;
}
.picListBox{
	width: 100%;
	height: 208rpx;
	margin-top: 24rpx;
	border: 1px solid #dddddd;
	background-color: #fff;
	white-space: nowrap;
	box-sizing: border-box;
	padding-top:20rpx;
}
.picListBox image{
	width: 168rpx;
	height: 168rpx;
	display: inline-block;
	margin-left: 24rpx;
}
.itemBox{
	width: 100%;
	margin-top: 24rpx;
}
.itemBox .title{
	font-size:28rpx;
	color:#333333;
	margin-bottom:12rpx;
}
.itemBox .item{
	width: 100%;
	height: 98rpx;
	background-color: #fff;
	border: 1px solid #dddddd;
	box-sizing: border-box;
	border-radius:12rpx;
	padding-left:30rpx;
	padding-right:30rpx;
	font-size:28rpx;
	color:#333333;
	line-height:98rpx;
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 78rpx;
}
.btnArea button{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #fd8238;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
}