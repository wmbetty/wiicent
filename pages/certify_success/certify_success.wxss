.titleBox{
	width: 100%;
	background-color: #fff;
	padding-top:82rpx;
	border-bottom:1px solid #dddddd;
	display:flex;
	flex-wrap:wrap;
	justify-content:center;
}
.titleBox .check{
	width: 120rpx;
	height: 120rpx;
}
.titleBox .title{
	width: 100%;
	text-align:center;
	font-size:40rpx;
	color:#333333;
	margin-top: 64rpx;
	margin-bottom:32rpx;
}
.titleBox .content{
	width: 100%;
	font-size: 26rpx;
	color: #999999;
	text-align: center;
}
.titleBox .btnBox{
	width:80%;
	display:flex;
	flex-wrap:nowrap;
	justify-content:space-around;
	margin-top:46rpx;
	padding-bottom:46rpx;
}
.titleBox .btnBox .btn{
	width:280rpx;
	height:68rpx;
	border:1px solid #dddddd;
	background-color:#fff;
	font-size:28rpx;
	text-align:center;
	color:#333333;
	line-height:68rpx;
	border-radius:12rpx;
}