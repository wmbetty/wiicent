.projectIntroBox{
	width: 100%;
}
.costCategoryBox{
	width: 100%;
    margin-top: 40rpx;
    border-top: 1px solid #dddddd;
}
.costCategoryBox .sessionBox{
	width: 100%;
	height: 90rpx;
	border-bottom: 1px solid #dddddd;
	box-sizing: border-box;
	position: relative;
	background-color: #fff;
	padding-left: 30rpx;
	padding-right: 30rpx;
}
.costCategoryBox .sessionBox::after{
	content: " ";
	display: block;
	clear: both;
	height: 0;
}
.costCategoryBox .sessionBox::before{
	content: " ";
	display: block;
	clear: both;
	height: 0;
}
.costCategoryBox .sessionBox .sessionVal{
	height: 90rpx;
	line-height: 90rpx;
	color: #333333;
	font-size: 28rpx;
	float: left;
}
.costCategoryBox .sessionBox .selectLogo{
	width: 32rpx;
	height: 32rpx;
	position: absolute;
	top: 24rpx;
	right: 30rpx;
	background-size: 32rpx 32rpx;
	background-repeat: no-repeat;
	background-image: url(data:image/png;base64,);
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 78rpx;
}
.btnArea view{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #fd8238;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
	border-radius: 20rpx;
}