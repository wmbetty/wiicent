.blackboard{
	width: 100%;
	height: 400rpx;
	border-bottom: 1px solid #dddddd;
	background-color: #fff;
	font-size: 28rpx;
	color: #333333;
	line-height: 48rpx;
  padding: 30rpx;
  box-sizing: border-box;
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 78rpx;
}
.btnArea button{
	width: 100%;
	height: 90rpx;
	line-height: 90rpx;
	background-color: #09ba79;
	text-align: center;
	font-size: 32rpx;
	color: #fff;
}