.item{
	width:100%;
	height:90rpx;
	position:relative;
	border-bottom:1px solid #dddddd;
	padding-left:30rpx;
	box-sizing:border-box;
	display:flex;
	align-items:center;
	background-color:#fff;
}
.item .label{
	font-size:28rpx;
	color:#333333;
}
.item image{
	width:44rpx;
	height:44rpx;
	position:absolute;
	top:23rpx;
	right:30rpx;
}