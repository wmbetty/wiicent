.session label{
	width: 100%;
	padding-left: 30rpx;
	box-sizing: border-box;
	height: 68rpx;
	line-height: 68rpx;
	color: #999999;
	font-size: 28rpx;
	margin-top: 20rpx;
  	display: block;
}
.session .itemBox{
	width: 100%;
	height: 90rpx;
	line-height: 90rpx;
	padding-left: 30rpx;
  	padding-right: 30rpx;
	box-sizing: border-box;
	font-size: 28rpx;
	color: #999999;
	border-top: 1px solid #dddddd;
	border-bottom: 1px solid #dddddd;
  	background-color: #fff;
  	position:relative;
}
.session textarea{
	width: 100%;
	height: 400rpx;
	padding: 30rpx;
	box-sizing: border-box;
	font-size: 32rpx;
	color: #333333;
	border-top: 1px solid #dddddd;
	border-bottom: 1px solid #dddddd;
  	background-color: #fff;
}
.session input{
	width: 100%;
	height: 86rpx;
	line-height:86rpx;
	font-size: 32rpx;
	color: #333333;
	box-sizing: border-box;
  	background-color: #fff;
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 78rpx;
	padding-bottom:200rpx;
}
.btnArea view{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #09ba79;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
	border-radius: 20rpx;
}
