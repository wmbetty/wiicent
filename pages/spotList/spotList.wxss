@import '../../component/nullTip/nullTip.wxss';

.spotListBox{
	width: 100%;
}
.spotListBox .spotItem{
	width: 100%;
	position: relative;
	height: 180rpx;
	border-bottom: 1px solid #dddddd;
	box-sizing: border-box;
	overflow:hidden;
	padding-left: 34rpx;
	padding-top: 34rpx;
  	background-color: #fff;
}
.spotListBox .spotItem::after{
	content:" ";
	display:block;
	height:0;
	clear:both;
}
.spotListBox .spotItem .arrowLogo{
	width: 32rpx;
	height: 32rpx;
	position: absolute;
	top: 76rpx;
	right: 30rpx;
	background-size: 32rpx 32rpx;
	background-repeat: no-repeat;
	background-image: url(data:image/png;base64,);
}
.spotListBox .contentBox{
	width:100%;
}
.spotListBox .spotItem .contentBox .title{
	width: 100%;
	height: 40rpx;
	line-height: 40rpx;
	font-size: 32rpx;
	color: #333333;
}
.spotListBox .spotItem .contentBox .distance{
	width: 100%;
	height: 28rpx;
	font-size: 28rpx;
	color: #999999;
	margin-bottom:30rpx;
}
.addLogo{
	width: 96rpx;
	height: 96rpx;
	border-radius: 48rpx;
	position: fixed;
	right: 58rpx;
	bottom: 106rpx;
	background-color: #09ba79;
	display:flex;
	justify-content:center;
	align-items:center;
}
.addLogo image{
	width:56rpx;
	height:56rpx;
}