.costCategoryBox{
	width: 100%;
    margin-top: 40rpx;
    border-top: 1px solid #dddddd;
}
.costCategoryBox .sessionBox{
	width: 100%;
	height: 90rpx;
	border-bottom: 1px solid #dddddd;
	box-sizing: border-box;
	position: relative;
	background-color: #fff;
	padding-left: 30rpx;
	padding-right: 30rpx;
}
.costCategoryBox .sessionBox::after{
	content: " ";
	display: block;
	clear: both;
	height: 0;
}
.costCategoryBox .sessionBox::before{
	content: " ";
	display: block;
	clear: both;
	height: 0;
}
.costCategoryBox .sessionBox .lIcon{
	width: 40rpx;
	height: 40rpx;
	float: left;
	margin-top: 26rpx;
	margin-right: 30rpx;
}
.costCategoryBox .sessionBox .sessionVal{
	height: 90rpx;
	line-height: 90rpx;
	color: #333333;
	font-size: 28rpx;
	float: left;
}
.costCategoryBox .sessionBox .selectLogo{
	width: 32rpx;
	height: 32rpx;
	position: absolute;
	top: 24rpx;
	right: 30rpx;
}
.costListBox{
    width: 100%;
    margin-top: 40rpx;
}
.costListBox .costBox{
	border-top: 1px solid #dddddd;
	border-bottom: 1px solid #dddddd;
	width: 100%;
	padding-left: 30rpx;
	background-color: #fff;
	box-sizing: border-box;
}
.costListBox .costBox input{
	width: 100%;
	height: 110rpx;
	line-height: 110rpx;
	font-size: 28rpx;
	color: #333333;
}
.costListBox .costBox .sessionBox{
	width: 100%;
	height: 90rpx;
	box-sizing: border-box;
	position: relative;
}
.costListBox .costBox .line{
	border-top: 1px solid #dddddd;
	border-bottom: 1px solid #dddddd;
}
.costListBox .costBox .sessionBox::after{
	content: " ";
	display: block;
	clear: both;
	height: 0;
}
.costListBox .costBox .sessionBox::before{
	content: " ";
	display: block;
	clear: both;
	height: 0;
}
.costListBox .costBox .sessionBox .lIcon{
	width: 40rpx;
	height: 40rpx;
	float: left;
	margin-top: 26rpx;
	margin-right: 30rpx;
	background-size: 40rpx 40rpx;
	background-repeat: no-repeat;
}
.costListBox .costBox .sessionBox .sessionVal{
	height: 90rpx;
	line-height: 90rpx;
	color: #333333;
	font-size: 28rpx;
	float: left;
}
.costListBox .costBox .sessionBox .sessionVal picker{
	float: left;
	margin-right:20rpx;
}
.costListBox .costBox .sessionBox .sessionVal .picker{
	height: 90rpx;
	line-height: 90rpx;
	color: #333333;
	font-size: 28rpx;
}
.costListBox .costBox .sessionBox .arrow{
	width: 34rpx;
	height: 34rpx;
	position: absolute;
	top: 24rpx;
	right: 30rpx;
}
.costListBox .remarkBox{
	width: 100%;
	margin-top: 20rpx;
	border-top: 1px solid #dddddd;
	border-bottom: 1px solid #dddddd;
}
.costListBox .remarkBox textarea{
	width: 100%;
	padding-left: 30rpx;
	padding-top: 30rpx;
	box-sizing: border-box;
	height: 197rpx;
	line-height: 34rpx;
	font-size: 28rpx;
	color: #999999;
	background-color: #fff;
}
.addLogo{
	width: 96rpx;
	height: 96rpx;
	border-radius: 48rpx;
	position: fixed;
	right: 58rpx;
	bottom: 106rpx;
	background-color: #09ba79;
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 78rpx;
}
.btnArea view{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #09ba79;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
	border-radius: 20rpx;
}
