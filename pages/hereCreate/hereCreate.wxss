.banner{
	width:100%;
	height:240rpx;
	background-color:#fff;
	over-flow:hidden;
}
.box{
	width:100%;
	border-top:1px solid #dddddd;
	border-bottom:1px solid #dddddd;
	padding-left:30rpx;
	box-sizing:border-box;
	background-color:#fff;
	margin-top:20rpx;
}
.box::after{
	content:'';
	width:100%;
	display:block;
	height:1px;
	background-color:#fff;
	margin-top:-2px;
}
.item{
	width:100%;
	height:90rpx;
	border-bottom:1px solid #dddddd;
	position:relative;
	display:flex;
	flex-wrap:nowrap;
	align-items:center;
}
.item .label{
	font-size:28rpx;
	color:#333333;
	white-space:nowrap;
}
.item .value{
	font-size:28rpx;
	color:#333333;
	width:100%;
	text-align:right;
	padding-right:74rpx;
	box-sizing:border-box;
}
.item .arrow{
	width:36rpx;
	height:36rpx;
	position:absolute;
	top:28rpx;
	right:26rpx;
}
.item .value1{
	width:100%;
	height:90rpx;
	line-height:90rpx;
	font-size:28rpx;
	color:#333333;
	padding-right:30rpx;
	box-sizing:border-box;
	text-align:right;
}
.item .padl{
	padding-right:128rpx;
}
.item .location{
	width:42rpx;
	height:44rpx;
	position:absolute;
	top:24rpx;
	right:36rpx;
}
.item .logoWrap{
	position:absolute;
	right:0;
	display:flex;
	flex-wrap:nowrap;
}
.item .logoBox{
	width:116rpx;
	height:90rpx;
	display:flex;
	justify-content:center;
	align-items:center;
}
.item .logoBox image{
	width:46rpx;
	height:46rpx;
}
.btnBox{
	width:100%;
	padding-left:30rpx;
	padding-right:30rpx;
	box-sizing:border-box;
	margin-top:60rpx;
}
.btnBox button{
	background-color:#09ba79;
	border-style:none;
	height:84rpx;
	line-height:84rpx;
	font-size:32rpx;
	color:#fff;
}
.picListBox{
	width: 100%;
	height: 208rpx;
	white-space: nowrap;
	box-sizing: border-box;
	padding-top:20rpx;
}
.picListBox image{
	width: 168rpx;
	height: 168rpx;
	display: inline-block;
}