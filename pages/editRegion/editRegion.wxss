.projectIntroBox{
	width: 100%;
}
.selectPositionBox{
	width: 100%;
	border-bottom: 1px solid #dddddd;
	padding-top:20rpx;
}
.selectPositionBox .itemBox{
	width: 100%;
	height: 90rpx;
	line-height: 90rpx;
	padding-left: 30rpx;
  	padding-right: 30rpx;
	box-sizing: border-box;
	font-size: 32rpx;
	color: #999999;
	border-top: 1px solid #dddddd;
  	background-color: #fff;
  	position:relative;
}
.selectPositionBox .itemBox .arrowLogo{
	width: 34rpx;
	height: 34rpx;
	position: absolute;
	top: 24rpx;
	right: 30rpx;
	background-size: 34rpx 34rpx;
	background-repeat: no-repeat;
	background-image: url(data:image/png;base64,);
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 78rpx;
}
.btnArea view{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #09ba79;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
	border-radius: 20rpx;
}
.selectPositionBox1{
	width: 100%;
	padding-top:40rpx;
}
.selectPositionBox1 .itemBox1{
	width: 100%;
	height: 90rpx;
	padding-left:30rpx;
	padding-right:30rpx;
	box-sizing: border-box;
	display:flex;
}
.selectPositionBox1 .itemBox1 input{
	width:100%;
	height:90rpx;
	line-height:90rpx;
	font-size: 32rpx;
	color: #333333;
	background-color: #fff;
	border-radius:12rpx;
	border:1px solid #dddddd;
	box-sizing:border-box;
	padding-left:20rpx;
}
.selectPositionBox1 .itemBox1 .textBtnBox{
	width:30%;
	height:90rpx;
	padding-left:30rpx;
	box-sizing:border-box;
}
.selectPositionBox1 .itemBox1 .textBtnBox button{
	width:100%;
	height:90rpx;
	line-height:90rpx;
	text-align:center;
	background-color:#fff;
	border:1px solid #09ba79;
	box-sizing:border-box;
	color: #09ba79;
	font-size:32rpx;
}