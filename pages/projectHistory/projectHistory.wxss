@import '../../component/nullTip/nullTip.wxss';
.lineWrap{
    width: 100%;
    height: 386rpx;
    overflow: hidden;
    background-color:#fff;
    margin-top: 24rpx;
    border-top:1px solid #dddddd;
    border-bottom:1px solid #dddddd;
}
.lineWrap .lineBox{
  width: 100%;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 30rpx;
  box-sizing: border-box;
  height: 330rpx;
}
.lineWrap .lineBox .lineInfo{
  width: 100%;
  border-bottom: 1px dashed #dddddd;
  box-sizing: border-box;
  position: relative;
  height: 105rpx;
}
.lineWrap .lineBox .lineInfo .touristPic{
  width: 74rpx;
  height: 74rpx;
  float: left;
  border-radius: 10rpx;
  margin-top: 4rpx;
}
.lineWrap .lineBox .lineInfo .nameAndXch{
  width: 450rpx;
  height: 74rpx;
  float: left;
}
.lineWrap .lineBox .lineInfo .nameAndXch .name{
  width: 100%;
  color: #333333;
  font-size: 30rpx;
  padding-left: 30rpx;
  margin-bottom: 10rpx;
}
.lineWrap .lineBox .lineInfo .nameAndXch .xch{
  width: 100%;
  color: #333333;
  font-size: 23rpx;
  padding-left: 30rpx;
}
.lineWrap .lineBox .lineInfo .lineState{
  width: 95rpx;
  height: 74rpx;
  float: right;
  line-height: 74rpx;
  color: #09ba79;
  font-size: 30rpx;
}
.lineWrap .lineBox .lineFromTo{
  width: 100%;
  position: relative;
  margin-top: 90rpx;
}
.lineWrap .lineBox .lineFromTo .from{
  width: 50%;
  height: 55rpx;
  line-height: 55rpx;
  font-size: 45rpx;
  color: #333333;
  float: left
}
.lineWrap .lineBox .lineFromTo .to{
  width: 50%;
  height: 55rpx;
  line-height: 55rpx;
  font-size: 45rpx;
  color: #333333;
  float: left;
  text-align: right;
}
.lineWrap .lineBox .lineFromTo .day{
  width: 230rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #333333;
  text-align: center;
  position: absolute;
  left: 50%;
  margin-left: -115rpx;
  border-bottom: 1px solid #dddddd;
}
.lineWrap .lineBox .lineFromTo .person{
  width: 230rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  color: #333333;
  text-align: center;
  position: absolute;
  top:50rpx;
  left: 50%;
  margin-left: -115rpx;
}
.lineWrap .lineBox .startEndDate{
  width: 100%;
  position: relative;
  top: 12rpx;
}
.lineWrap .lineBox .startEndDate .startDate{
  width: 50%;
  height: 55rpx;
  line-height: 55rpx;
  font-size: 28rpx;
  color: #333333;
  float: left
}
.lineWrap .lineBox .startEndDate .endDate{
  width: 50%;
  height: 55rpx;
  line-height: 55rpx;
  font-size: 28rpx;
  color: #333333;
  float: left;
  text-align: right;
}
.queryTabBox{
  width: 100%;
  height: 102rpx;
  background-color:#fff;
  border-top:1px solid #dddddd;
  border-bottom:1px solid #dddddd;
  padding-top:32rpx;
  box-sizing:border-box;
  position:fixed;
  top:104rpx;
}
.queryTabBox::after{
  content:'';
  display:block;
  clear:both;
}
.queryTabBox .queryTab{
  width:33.33%;
  float:left;
  display:flex;
  justify-content:center;
  align-items:center;
  border-left:1px solid #dddddd;
  box-sizing:border-box;
  margin-left:-1px;
}
.queryTabBox .queryTab .text{
  font-size:28rpx;
  color:#999999;
  margin-right:12rpx;
}
.queryTabBox .queryTab image{
  width:14rpx;
  height:10rpx;
}
.queryTabPanel{
  width:100%;
  position:fixed;
  top:206rpx;
  z-index:10;
}
.queryTabPanel .shadow{
  width:100%;
  height:100%;
  background-color:rgba(0,0,0,0.3);
  position:absolute;
  top:0;
}
.queryTabPanel .queryStateList{
  width:100%;
  background-color:#fff;
  position:absolute;
  top:0;
  z-index:10;
  padding-left:30rpx;
  padding-right:30rpx;
  box-sizing:border-box;
}
.queryTabPanel .queryStateList .queryStateItem{
  width:100%;
  height:98rpx;
  position:relative;
  line-height:98rpx;
  font-size:28rpx;
  color:#333333;
  border-bottom:1px solid #dddddd;
}
.queryTabPanel .queryStateList .queryStateItem image{
  width:42rpx;
  height:42rpx;
  position:absolute;
  top:28rpx;
  right:0;
}
.searchBox{
  width: 100%;
  height: 104rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 22rpx;
  box-sizing: border-box;
  position: relative;
  position:fixed;
  top:0;
}
.searchBox image{
  width: 28rpx;
  height: 28rpx;
  position: absolute;
  top: 40rpx;
  left:54rpx;
}
.searchBox input{
  width: 100%;
  height: 60rpx;
  border: 1px solid #dddddd;
  border-radius: 30rpx;
  background-color: #fff;
  padding-left: 90rpx;
  box-sizing: border-box;
}