.projectIntroBox{
	width: 100%;
}
.projectIntroBox .session label{
	width: 100%;
	padding-left: 30rpx;
	box-sizing: border-box;
	height: 68rpx;
	line-height: 68rpx;
	color: #999999;
	font-size: 24rpx;
	margin-top: 20rpx;
  	display: block;
}
.projectIntroBox .session input{
	width: 100%;
	height: 90rpx;
	line-height: 90rpx;
	padding-left: 30rpx;
  	padding-right: 30rpx;
	box-sizing: border-box;
	font-size: 32rpx;
	color: #333333;
	border-top: 1px solid #dddddd;
	border-bottom: 1px solid #dddddd;
  	background-color: #fff;
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 78rpx;
}
.btnArea button{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #fd8238;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
	border-radius: 20rpx;
}