page{
	display: block;
	min-height: 100%;
	background-color:#fff;
}
.scheduleDetail{
	width:100%;
	padding-left: 20rpx;
	padding-right: 20rpx;
	padding-bottom:600rpx;
	box-sizing: border-box;
}
.scheduleDetail .title{
	width: 100%;
	padding-top: 34rpx;
	font-size: 28rpx;
	color: #fd8238;
	line-height: 34rpx;
}
.scheduleDetail .content{
	width: 100%;
	padding-top: 30rpx;
	font-size: 24rpx;
	color: #999999;
	line-height: 54rpx;
	padding-bottom:20rpx;
	word-break:break-all;
}
.scheduleDetail .list{
	width: 100%;
	height: 72rpx;
	line-height: 72rpx;
	font-size: 28rpx;
	color: #333333;
}
.btnArea{
	width: 100%;
	padding-left: 30rpx;
	padding-right: 30rpx;
	box-sizing: border-box;
	margin-top: 78rpx;
}
.btnArea view{
	width: 100%;
	height: 96rpx;
	line-height: 96rpx;
	background-color: #fd8238;
	text-align: center;
	font-size: 36rpx;
	color: #fff;
	border-radius: 20rpx;
}
.addLogo{
	width: 96rpx;
	height: 96rpx;
	border-radius: 48rpx;
	position: fixed;
	right: 58rpx;
	bottom: 106rpx;
	background-color: #ff8c00;
	background-size:96rpx 96rpx;
	background-repeat:no-repeat;
	overflow:hidden;
	background-image: url(data:image/png;base64,);
}