page{
	background-color:#fff;
}
.personDetail .person{
	width:100%;
	height:184rpx;
	position:relative;
	background-color:#e5e5e5;
	border-bottom:1px solid #dddddd;
}
.personDetail .person image{
	width:144rpx;
	height:144rpx;
	border-radius:72rpx;
	position:absolute;
	top:20rpx;
	left:30rpx;
}
.personDetail .person .title1{
	font-size:28rpx;
	color:#333333;
	position:absolute;
	top:40rpx;
	left:200rpx;
}
.personDetail .person .title2{
	font-size:24rpx;
	color:#999999;
	position:absolute;
	top:110rpx;
	left:200rpx;
}
.personDetail .session{
	width:100%;
	height:120rpx;
	padding-left:30rpx;
	padding-right:30rpx;
	box-sizing:border-box;
	border-bottom:1px solid #dddddd;
	display:flex;
	align-items:center;
}
.personDetail .session view{
	fontsize:28rpx;
	color:#999999;
	margin-right:30rpx;
}
.btnBox{
	width:100%;
	padding-left:30rpx;
	padding-right:30rpx;
	box-sizing:border-box;
}
.btnBox .btn{
	width: 100%;
	height: 76rpx;
	line-height: 76rpx;
	background-color:#fc8145;
	text-align:center;
	font-size: 28rpx;
	color:#fff;
	border-radius:12rpx;
	margin-top:94rpx;
}